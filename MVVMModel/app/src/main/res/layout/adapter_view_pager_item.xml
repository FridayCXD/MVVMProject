<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data class="ViewPager2AdapterBinding">
    </data>

    <LinearLayout
        android:id="@+id/viewPager2ItemFrameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:showDividers="middle"
        android:dividerPadding="@dimen/dp_5"
        android:padding="@dimen/dp_20"
        android:orientation="vertical">
        <com.mvvm.model.widget.DrawableTextView
            android:id="@+id/itemTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:drawablePadding="@dimen/dp_3"
            android:background="@drawable/tag_oval_bg_shape"
            android:paddingTop="@dimen/dp_3"
            android:paddingBottom="@dimen/dp_3"
            app:leftDrawable="@drawable/ic_location_on_black_24dp"
            app:leftDrawableWidth="@dimen/dp_15"
            app:leftDrawableHeight="@dimen/dp_15"
            android:paddingStart="@dimen/dp_20"
            android:paddingEnd="@dimen/dp_25"
            tools:text="ViewPager2çš„item"
            />
        <Button
            android:id="@+id/verticalBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/vertical"/>
    </LinearLayout>
</layout>